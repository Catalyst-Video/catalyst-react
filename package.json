{
  "name": "catalyst-vc-react",
  "version": "1.4.3",
  "author": "@GoldinGuy seth@goldin.io & @JoeSemrai josephsemrai@gmail.com",
  "private": false,
  "license": "AGPL-3.0-only",
  "description": "An awesome video-chat react component that can be added to your site in fewer than 5 lines of code",
  "repository": "https://github.com/Catalyst-Video/catalyst-react",
  "bugs": "https://github.com/Catalyst-Video/catalyst-react/issues",
  "keywords": [
    "video chat",
    "react video chat",
    "react",
    "free video chat",
    "socket-io",
    "webrtc"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build:tailwind": "tailwindcss build src/styles/tailwind.css -o src/styles/tailwind.output.css -w",
    "build:tailwind:prod": "tailwindcss build src/styles/tailwind.css -o src/styles/tailwind.output.css",
    "prestart": "npm run build:tailwind",
    "prebuild": "npm run build:tailwind:prod",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "detectrtc": "^1.4.1",
    "react-draggable": "^4.4.3",
    "react-full-screen": "^1.0.1",
    "react-scripts": "4.0.3",
    "socket.io-client": "^2.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-strip": "^2.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "@size-limit/preset-big-lib": "^4.10.1",
    "@size-limit/preset-small-lib": "^4.10.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/socket.io-client": "^1.4.36",
    "autoprefixer": "^9.8.6",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "lint": "^0.7.0",
    "postcss": "^7.0.35",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "rollup-plugin-copy": "github:Catalyst-Video/rollup-plugin-copy#master",
    "rollup-plugin-postcss": "^4.0.0",
    "size-limit": "^4.10.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/catalyst-vc-react.esm.js",
  "size-limit": [
    {
      "path": "dist/catalyst-vc-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/catalyst-vc-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "jest": {
    "testEnvironment": "jest-environment-jsdom-fifteen",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  }
}
