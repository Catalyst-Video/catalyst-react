{
  "name": "catalyst-vc-react",
  "version": "3.0.0",
  "author": "@GoldinGuy seth@goldin.io & @JoeSemrai josephsemrai@gmail.com",
  "private": false,
  "license": "AGPL-3.0-only",
  "description": "Reliable and scalable open-source video chat in a few lines of code",
  "repository": "https://github.com/Catalyst-Video/catalyst-react",
  "bugs": "https://github.com/Catalyst-Video/catalyst-react/issues",
  "keywords": [
    "video chat",
    "react video chat",
    "react",
    "free video chat",
    "socket-io",
    "webrtc",
    "catalyst",
    "livekit",
    "video-chat",
    "embed video chat"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prestart:tailwind": "npx tailwindcss-cli@latest build src/styles/tailwind.css -o src/styles/tailwind.output.css -w",
    "prebuild:tailwind": "npx tailwindcss-cli@latest build src/styles/tailwind.css -o src/styles/tailwind.output.css",
    "prestart": "npm run prestart:tailwind",
    "prebuild": "npm run prebuild:tailwind",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@types/css-global-variables": "^3.0.1",
    "catalyst-client": "Catalyst-Video/catalyst-client#master",
    "css-global-variables": "^3.0.2",
    "livekit-client": "^0.9.7",
    "lodash.debounce": "^4.0.8",
    "plain-draggable": "^2.5.14",
    "react-full-screen": "^1.0.1",
    "react-intersection-observer": "^8.32.0",
    "react-scripts": "^1.1.5",
    "react-tiny-popover": "^6.0.5",
    "ts-debounce": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-strip": "^2.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "@size-limit/preset-big-lib": "^4.5.5",
    "@size-limit/preset-small-lib": "^4.5.5",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/autoprefixer": "^10.2.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/socket.io-client": "^1.4.36",
    "@types/tailwindcss": "^2.0.3",
    "autoprefixer": "^9.8.6",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "lint": "^0.7.0",
    "postcss": "^8.3.5",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.5",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "rollup-plugin-postcss": "^1.6.3",
    "size-limit": "^4.10.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/catalyst-vc-react.esm.js",
  "size-limit": [
    {
      "path": "dist/catalyst-vc-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/catalyst-vc-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "jest": {
    "testEnvironment": "jest-environment-jsdom-fifteen",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  }
}
